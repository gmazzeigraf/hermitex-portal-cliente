/* Galleria Produtos */
.galleria-produtos {
	border: 0px !important;
}

.galleria-produtos img:hover {
	transform: scale(1.5); /* (150% zoom - Note: if the zoom is too large, it will go outside of the viewport) */
}
/* 	Correcao da altura da imagem da galleria */
.ui-galleria, .ui-galleria-panel-wrapper, .ui-galleria-panel {
	min-height: 200px;
}

.ui-galleria-panel img {
	max-height: 100% !important;
}

/* Galleria nao ultrapassar a largura maxima */
.ui-galleria, .ui-galleria-panel-wrapper, .ui-galleria-panel, .ui-galleria-panel img {
	max-width: 100% !important; text-align: center;
}

