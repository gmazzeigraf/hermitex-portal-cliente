@media screen and (min-width: 1025px) {
	.coluna-edicao {
		width: 50px;
	}
	.display-mobile {
		display: none !important;
	}
	.route-bar {
		margin-top: 5px !important; line-height: 40px !important;
	}
	.layout-menu-container {
		border-top: none !important;
	}
	.lista-pick .ui-picklist-list {
		height: 650px !important;
	}
	.login-panel {
		position: relative !important; margin: 0 auto !important; width: 350px !important;
	}
	.id-column {
		width: 100px !important; text-align: center !important;
	}
	.status-column {
		width: 150px !important; text-align: center !important;
	}
	.valor-column {
		width: 150px !important;
	}
	.data-column {
		width: 200px !important;
	}
	.resp-calendar input.hasDatepicker {
		width: 83%;
	}
	.layout-wrapper .topbar .topbar-right .topbar-items>li>a .topbar-item-name-mobile {
		display: none !important;
	}
	.topbar-item-name-web span {
		display: block !important;
	}
	.layout-main {
		min-height: 92vh !important; box-sizing: border-box !important;
	}
	.layout-main-content {
		padding-top: 0px !important;
	}
	.imagem-capa {
		text-align: center;
	}
	.coluna-valor {
		text-align: center; width: 200px;
	}
	.coluna-center {
		text-align: center;
	}
	/* 	Botoes quantidade item */
	.botao-aumenta-quantidade-item {
		margin-left: 5px !important;
	}
	.botao-diminui-quantidade-item {
		margin-right: 5px !important;
	}
	.produtos .ui-panel-content {
		min-height: 300px !important;
	}
	.produtos img {
		height: 180px !important;
	}
	.video-youtube {
		width: 350px !important; height: 262px;
	}
	.resp-dialog {
		min-height: 20vh !important; max-height: 70vh !important;
	}
}

@media ( max-width : 1024px) {
	.display-desktop {
		display: none !important;
	}
	.resp-dialog {
		max-height: 50vh !important;
	}
	.layout-wrapper .topbar .topbar-right .topbar-items>li>a .topbar-item-name-mobile {
		display: inline-block !important; vertical-align: middle !important;
	}
	.topbar-item-name-web {
		display: none !important;
	}
	.layout-main {
		min-height: 87vh !important; box-sizing: border-box !important;
	}
	.imagem-capa {
		text-align: center; width: 100% !important;
	}
	.video-youtube {
		width: 100%;
	}
}

@media ( max-width : 1245px) {
	.resp-calendar input.hasDatepicker {
		width: 75%;
	}
}

.coluna-edicao {
	text-align: center;
}

